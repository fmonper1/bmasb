<p id="notice"><%= notice %></p>

<h1>Shops</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Dir1</th>
      <th>Dir2</th>
      <th>Dir3</th>
      <th>Dir4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shops.each do |shop| %>
      <tr>
        <td><%= shop.titulo %></td>
        <td><%= shop.dir1 %></td>
        <td><%= shop.dir2 %></td>
        <td><%= shop.dir3 %></td>
        <td><%= shop.dir4 %></td>
        <td><%= link_to 'Show', shop %></td>
        <td><%= link_to 'Edit', edit_shop_path(shop) %></td>
        <td><%= link_to 'Destroy', shop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shop', new_shop_path %>
