<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Descripcion</th>
      <th>Fecha contacto</th>
      <th>Nombre contacto</th>
      <th>Numero contacto</th>
      <th>Tienda</th>
      <th>Tipo</th>
      <th>Estado</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.titulo %></td>
        <td><%= event.descripcion %></td>
        <td><%= event.fecha_contacto %></td>
        <td><%= event.nombre_contacto %></td>
        <td><%= event.numero_contacto %></td>
        <td><%= event.shop.titulo %></td>
        <td><%= event.type.titulo %></td>
        <td><%= event.state.titulo %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
